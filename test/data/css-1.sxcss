(
 (style-rule body
             (padding-left (11 em))
             (font-family (or Georgia "Times New Roman" Times serif))
             (color purple)
             (background-color (color "d8da3d")))
 (style-rule (ul (class navbar))
             (list-style-type none)
             (padding 0)
             (margin 0)
             (position absolute)
             (top (2 em))
             (left (1 em))
             (width (9 em)))
 (style-rule h1
             (font-family (or Helvetica Geneva Arial SunSans-Regular sans-serif)))
 (style-rule ((ul (class navbar)) li)
             (background white)
             (margin #((0.5 em) 0))
             (padding (0.3 em))
             (border-right #((1 em) solid black)))
 (style-rule ((ul (class navbar)) a)
             (text-decoration none))
 (style-rule (a (: link))
             (color blue))
 (style-rule (a (: visited))
             (color purple))
 (style-rule address
             (margin-top (1 em))
             (padding-top (1 em))
             (border-top #(thin dotted)))
 )
